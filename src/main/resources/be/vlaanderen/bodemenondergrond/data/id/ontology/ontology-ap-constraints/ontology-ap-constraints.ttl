beo_ont:ontology
    a               owl:Ontology ;
    rdfs:comment
                    "Specifieert hoe de data in de ontologie van bodem en ondergrond te encoderen in RDF (i.e. RDF termen, kardinaliteiten,...)."@en ;
    rdfs:label      "ontologys AP Constraints Limited"@en ;
    dct:abstract    "Het ontologys applicatieprofiel specifieert de RDF termen (klassen en properties) en bijhorende constraints die gebruikt worden voor het beschrijven van ontologys. Het maakt hierbij zoveel mogelijk gebruik van bestaande termen (klassen en properties) uit bestaande , skos, dbo, dbp en een minimale set van nieuwe termen gedefinieerd in het ontologys Vocabularium."@en ;
    dct:alternative "ontologys AP Shapes"@en ;
    dct:creator     beo_ag_cl:creator ;
    dct:description "Dit is de SHACL encodering van het applicatieprofiel van een ontologys dat de eerste stap van de transformatie vanuit csv controleert."@en ;
    dct:issued      "2023-10-23"^^xsd:date ;
    dct:title       "ontologys AP Constraints Limited"@en ;
    xkos:belongsTo  beo_dataset:ontology ;
    xhv:last        beo_voc:ontology ;
    owl:sameAs      beo_voc:ontology ;
    owl:versionInfo "Versie 3.0.0  maart 2024"@en .


beo_voc:ontology
    a          owl:Ontology ;
    rdfs:label "ontology AP Constraints"@en ;
    owl:sameAs beo_ont:ontology .

beo_ag_cl:creator
    rdfs:label "Ontwikkelaar ontology"@en ;
    schema:affiliation vl_org:OVO003749 ;
    a foaf:Person;
    foaf:firstName "Geert";
    foaf:lastName "Van Haute";
    foaf:mbox <mailto:geert.vanhaute@vlaanderen.be>.

# Node Shapes
ons:schema
    a                sh:NodeShape ;
    rdfs:label       "conceptschema van matrices"@en ;
    sh:name          "conceptschema van matrices"@en ;
    sh:description   "Het schema waarbinnen de matrices gedefinieerd zijn"@en ;
    rdfs:isDefinedBy beo_ont:ontology, beo_voc:ontology ;
    sh:property      omg_ps:conceptscheme_prefLabel, omg_ps:schema_hasTopConcept, omg_ps:dc_identifier, omg_ps:notation, omg_ps:note,
                     omg_ps:definition, omg_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:ConceptScheme .



ons:collection
    a                sh:NodeShape ;
    rdfs:label       "verzameling"@en ;
    sh:name          "verzameling"@en ;
    sh:description   "Verzamelingen bundelen matrices met gelijkaardige eigenschappen, analysemethoden of matrices waarin ze gemeten worden."@en ;
    rdfs:isDefinedBy beo_ont:ontology, beo_voc:ontology ;
    sh:property      omg_ps:prefLabel, omg_ps:collection_member, omg_ps:dc_identifier, omg_ps:notation,
                     omg_ps:inScheme, omg_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:Collection .



ons:concept
    a                sh:NodeShape ;
    rdfs:label       "matrix"@en ;
    sh:name          "matrix"@en ;
    sh:description   "analysemethodes"@en ;
    rdfs:comment     "compendia onderhouden door VITO"@en ;
    rdfs:isDefinedBy beo_ont:ontology, beo_voc:ontology ;
    sh:property      omg_ps:notation, omg_ps:inScheme,
                     omg_ps:references, omg_ps:definition,
                     omg_ps:prefLabel, omg_ps:theme,
                     omg_ps:topConceptOf, omg_ps:altLabel, omg_ps:dc_identifier,
                     omg_ps:rdf_type, omg_ps:note, omg_ps:broaderTransitive, omg_ps:broader,
                     omg_ps:narrowerTransitive, omg_ps:narrower, omg_ps:semanticRelation  ;
    sh:closed true ;
    sh:targetClass   skos:Concept .



# Property Shapes

omg_ps:rdf_type
    a                sh:PropertyShape ;
    rdfs:label       "rdf type"@en ;
    sh:name          "rdf type"@en ;
    sh:description   "Aanduiding dat een bron een instantie van een bepaalde klasse is."@en ;
    rdfs:isDefinedBy beo_ont:ontology, beo_voc:ontology ;
    sh:maxCount      1 ;
    sh:minCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          rdf:type .


omg_ps:topConceptOf
